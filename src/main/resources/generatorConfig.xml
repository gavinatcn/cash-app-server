<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
		PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
		"http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
	<classPathEntry location="/Users/gavin/.m2/repository/mysql/mysql-connector-java/5.1.24/mysql-connector-java-5.1.24.jar" />

	<context id="MysqlTables" targetRuntime="MyBatis3">

		<commentGenerator>
			<property name="suppressDate" value="true" />
		</commentGenerator>

		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
		                        connectionURL="jdbc:mysql://127.0.0.1:3306" userId="yumi"
		                         password="yumi">
		</jdbcConnection>

		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<javaModelGenerator targetPackage="com.yumi.cash.app.server.dao"
							targetProject="src/main/java">
			<property name="enableSubPackages" value="false" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>

		<sqlMapGenerator targetPackage="com.yumi.cash.app.server.mapper"
						 targetProject="src/main/resources">
			<property name="enableSubPackages" value="false" />
		</sqlMapGenerator>

		<javaClientGenerator type="XMLMAPPER"
							 targetPackage="com.yumi.cash.app.server.dal" targetProject="src/main/java">
			<property name="enableSubPackages" value="false" />
		</javaClientGenerator>

		<!-- 配置所需要生成的表 -->
		<table catalog="yumi-cash" schema="yumi-cash" tableName="PRODUCT_BASIC_INFO" domainObjectName="ProductBasicInfo"></table>

	</context>

</generatorConfiguration>
